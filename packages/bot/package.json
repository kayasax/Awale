{
  "name": "@awale/bot",
  "version": "0.1.0",
  "main": "dist/bot/src/index.js",
  "types": "dist/bot/src/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
  "bundle": "esbuild src/server.ts --platform=node --format=cjs --bundle --sourcemap --external:dtrace-provider --outfile=dist/server.bundle.cjs",
    "build:all": "npm run build && npm run bundle",
    "start": "node dist/server.bundle.cjs",
    "dev": "nodemon --exec ts-node src/server.ts"
  },
  "dependencies": {
    "botbuilder": "^4.22.0",
    "restify": "^11.1.0"
  },
  "devDependencies": {
    "@types/restify": "^8.5.12",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.1"
    ,"esbuild": "^0.21.5"
  }
}
